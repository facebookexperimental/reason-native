{
  "name": "@reason-native/cli",
  "version": "0.0.1-alpha",
  "description": "Build Command Line Interfaces in Reason",
  "author": "Facebook Engineering",
  "homepage": "https://reason-native.com",
  "esy-prepublish-generate-opam": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/facebookexperimental/reason-native"
  },
  "license": "MIT",
  "keywords": ["cli", "reasonml", "reason", "ocaml", "esy"],
  "esy": {
    "build": "dune build -p cli"
  },
  "scripts": {
    "release": "node ./scripts/esy-prepublish.js cli.json"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@opam/re": "*",
    "@reason-native/pastel": "jfrolich/reason-native:pastel.json",
    "@opam/reason": ">= 3.6.0",
    "ocaml": "^4.2.0"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  },
  "resolutions": {
    "@reason-native/pastel": "./pastel.json"
  }
}
