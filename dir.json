  },
  "devDependencies": {
    "@reason-native/fp": "./fp.json",
    "@opam/merlin": "*",
    "ocaml": "~4.6.0",
    "@reason-native/rely": "./rely.json"
  },
  "resolutions": {
    "@reason-native/pastel": "./pastel.json",
    "@reason-native/cli": "./cli.json",
    "@reason-native/file-context-printer": "./file-context-printer.json"
  }
